{
  "name": "AUD-Scout-Nikethan-Rai",
  "displayName": "AUD-Scout",
  "description": "An extension that creates softlink for different kinds in your AUD. Authored by Nikethan Rai",
  "version": "0.0.1",
  "publisher": "Nikethan",
  "icon": "icon.png",
  "engines": {
    "vscode": "^1.92.0"
  },
  "categories": [
